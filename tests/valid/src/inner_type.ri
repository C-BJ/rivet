struct OuterStruct {
    struct InnerStruct {
        abc: int32;
    }

    inner: InnerStruct;

    func method(self) { }

    func associated_fn() { }

    func caller(self) {
        self.method();
        OuterStruct.associated_fn();
    }
}

test "inner type" {
    o := OuterStruct(inner: OuterStruct.InnerStruct(5));
    @assert(o.inner.abc == 5);
}
