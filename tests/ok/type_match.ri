union Any {
    i32, str;
}

fn main() {
    let mut any = Any("String");
    match (any) is {
        str as any_s => assert!(any_s == "String"),
        else => assert!(false)
    }

    any = Any(5);
    match (any) is {
        i32 as any_i => assert!(any_i == 5),
        else => assert!(false)
    }
}
