class OuterClass {
    class InnerClass {
        abc: i32;
    }

    inner: InnerClass;

    fn method(self) { }

    fn associated_fn() { }

    fn caller(self) {
        self.method();
        OuterClass.associated_fn();
    }
}

test "inner type" {
    let o = OuterClass(inner: OuterClass.InnerClass(5));
    @assert(o.inner.abc == 5);
}
