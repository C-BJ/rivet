#if _LINUX_
extern (C) {
    public fn __errno_location() ?*mut i32;
}

public const EDOM: i32 = 33;
public const EILSEQ: i32 = 84;
public const ERANGE: i32 = 34;
#endif

#if _WINDOWS_
extern (C) {
    #[dllimport]
    public fn _errno() *mut i32;
}

public const EDOM: i32 = 33;
public const EILSEQ: i32 = 42;
public const ERANGE: i32 = 34;
#endif
