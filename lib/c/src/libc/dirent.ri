pub struct dirent {
    pub d_ino: usize;
    pub d_off: usize;
    pub d_reclen: u16;
    pub d_type: u8;
    pub d_name: [256]u8;
}

extern (C) {
	pub fn opendir(d: *u8) ?*dirent;
	pub fn readdir(d: *dirent) ?*dirent;
	pub fn closedir(d: *dirent) i32;
}
