public struct dirent {
    public d_ino: usize;
    public d_off: usize;
    public d_reclen: u16;
    public d_type: u8;
    public d_name: [256]u8;
}

extern (C) {
	public fn opendir(d: *u8) ?*dirent;
	public fn readdir(d: *dirent) ?*dirent;
	public fn closedir(d: *dirent) i32;
}
